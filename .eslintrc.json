{
        "root": true,
        "plugins": [
        ],
        "env": {
            "browser": true,
            "amd": true,
            "worker": true,
            "es6": true
        },
        "ecmaFeatures": {
            "modules": true,
            "jsx": false
        },
        "extends": "eslint:recommended",

    "rules": {
        "space-before-function-paren": 0,
        "one-var": 0,
        "indent": ["error", "tab"],
        "no-unused-vars": 1,
        "no-debugger": 2,
        "no-dupe-keys": 2,
        "no-console": 0,
        "no-tabs": 0,
        "max-nested-callbacks": ["warn", 5],
        "semi": ["error", "always"],
        "prefer-template": 0,
        "no-nested-ternary": 0
    }

}
